    @RenderBody()
    <footer class="container-fluid">
        <div class="container text-center">
            <div class="col-sm-2 col-sm-push-10 text-center">
                <a href="#Home" class="regress text-center internalLink">
                    <img width="30" src="img/Go_top@2x.png">
                    <br>Back To Top
                </a>
            </div>
            <div class="col-sm-8 text-center">
                <p>Created with the community for our communities. <br> &copy; @DateTime.UtcNow.Year Solve Chicago. All Rights Reserved.</p>
            </div>
            <div class="col-sm-2 col-sm-pull-10">
                <img src="img/logo_white@2x.png" height="45" alt="">
            </div>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/functions")
    @RenderSection("scripts", required: false)
    
